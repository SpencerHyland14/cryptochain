ed":{"line":120,"column":15},"source":"components/ConductTransaction.js","original":{"line":66,"column":15}}],"sources":{"components/ConductTransaction.js":"import React, {Component} from 'react';\nimport {FormGroup, FormControl, Button} from 'react-bootstrap';\nimport{Link} from 'react-router-dom';\nimport history from '../history';\n\nclass ConductTransaction extends Component{\n    state = {recipient: '', amount: 0};\n\n    updateRecipient = event => {\n        this.setState({recipient: event.target.value});\n    }\n\n    updateAmount = event => {\n        this.setState({amount: Number(event.target.value)});\n    }\n\n    conductTransaction = () =>{\n        const {recipient, amount} = this.state;\n\n        fetch(`${document.location.origin}/api/transact`, {\n            method: 'POST',\n            headers: {'Content-Type': 'application/json'},\n            body: JSON.stringify({recipient, amount})\n        }).then( response => response.json())\n        .then(json => {\n            alert(json.message || json.type);\n            history.push('/transaction-pool');\n        });\n    }\n\n    render(){\n\n        return(\n            <div className = 'ConductTransaction'>\n                <Link to ='/'>Home</Link>\n                <h3>Conduct a Transaction</h3>\n                <FormGroup>\n                    <FormControl \n                        input = 'text'\n                        placeholder = 'recipient'\n                        value = {this.state.recipient}\n                        onChange = {this.updateRecipient}\n                    />\n                </FormGroup>\n                <FormGroup>\n                <FormControl \n                        input = 'number'\n                        placeholder = 'amount'\n                        value = {this.state.amount}\n                        onChange = {this.updateAmount}\n                    />\n                </FormGroup>\n                <div>\n                    <Button\n                        bsStyle = \"danger\"\n                        onClick = {this.conductTransaction}\n                    >\n                        Submit\n                    </Button>\n                </div>\n            </div>\n        )\n    }\n};\n\nexport default ConductTransaction;"},"lineCount":null}},"error":null,"hash":"08157d8811abe1242e9c94b20ea8d747","cacheData":{"env":{}}}